#include <iostream>
#include <cstring>
#include <algorithm>
using namespace std;

#define max 2000
char s[max],t[max];
int dp[max][max];

int main()
{
 	cin >> s >> t;
	int len1 = strlen(s);
	int len2 = strlen(t);
	for(int i=1; i <= len1; i++)   //初始化
		dp[i][0] = i;
	for(int i=1; i<=len2; i++)
		dp[0][i] = i;
		
	
	for(int i = 1; i <= len1; i++)
	{
		for(int j = 1; j <= len2 ; j++)
		{
			dp[i][j] = min(dp[i-1][j-1]+(s[i-1] == t[j-1]?0:1),min(dp[i-1][j]+1,dp[i][j-1]+1)); 
		}
	}
	
	if(dp[len1][len2] <= 2) 
		cout << "YES" <<endl;
		
	else
		cout <<"NO" << endl;
		
		return 0;
}
