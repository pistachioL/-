#include <iostream>
#include <stdio.h>
#include <cstring>
using namespace std;

int main()
{
    int n,len;
    cin >> n;

    char a[250],b[250];
    getchar(); //从缓冲区读取cin留下的'\n'，否则gets（）会读取有误
    while(n--)
    {
        gets(a);
        len = strlen(a);
        for(int i=0;i<len;i++)
        {
            b[len-i-1] = a[i];
        }

        for(int i=0;i<len;i++)
        {
            cout << b[i];
        }
    }
    return 0;
}
